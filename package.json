{
  "name": "basic-boilerplate",
  "version": "0.0.1",
  "description": "Stylus/Webpack/Babel/ESLint boilerplate",
  "main": "src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --inine --hot"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "es6": true,
    "browser": true,
    "rules": {
      "no-cond-assign": 2,
      "curly": [
        2,
        "multi-line"
      ],
      "eqeqeq": 2,
      "no-multi-spaces": 2,
      "brace-style": [
        2,
        "1tbs"
      ],
      "indent": [
        2,
        2
      ]
    },
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "author": "Art Gillespie <art@tapsquare.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.3.1",
    "autoprefixer-stylus": "^0.9.1",
    "babel": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "postcss": "^5.0.14",
    "postcss-loader": "^0.8.0",
    "react": "^0.14.7",
    "react-core": "0.0.0",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0-rc5",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
